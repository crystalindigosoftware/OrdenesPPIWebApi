/****** Object:  Table [dbo].[Ordenes]    Script Date: 27/11/2024 11:02:48 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE TABLE [dbo].[Ordenes](
	[Id] [int] IDENTITY(1,1) NOT NULL,
	[CuentaId] [int] NOT NULL,
	[ActivoId] [int] NOT NULL,
	[EstadoId] [int] NOT NULL,
	[Operacion] [varchar](5) NOT NULL,
	[Cantidad] [decimal](10, 4) NOT NULL,
	[MontoTotal] [decimal](10, 4) NOT NULL,
	[Comisiones] [decimal](10, 4) NOT NULL,
	[Impuestos] [decimal](10, 4) NOT NULL,
	[Monto] [decimal](10, 4) NOT NULL,
 CONSTRAINT [PK_Ordenes] PRIMARY KEY CLUSTERED 
(
	[Id] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON, OPTIMIZE_FOR_SEQUENTIAL_KEY = OFF) ON [PRIMARY]
) ON [PRIMARY]
GO
/****** Object:  View [dbo].[V_Ordenes]    Script Date: 27/11/2024 11:02:48 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE VIEW [dbo].[V_Ordenes] AS SELECT O.Id As IdOrden,C.Id  As IdCuenta, C.Descripcion As NombreCuenta, A.Nombre As NombreActivo, O.Cantidad,A.PrecioUnitario As Precio,E.descripcionEstado As Estado,O.MontoTotal, O.Operacion FROM Ordenes O INNER JOIN Cuentas C ON C.Id = O.CuentaId INNER JOIN Activos A ON A.Id = O.ActivoId INNER JOIN Estados E ON E.Id = O.EstadoId; 
GO
